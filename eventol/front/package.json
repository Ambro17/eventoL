{
  "name": "links",
  "version": "0.0.1",
  "scripts": {
    "start": "node server.js",
    "test": "./node_modules/.bin/jest --forceExit --ci --coverage",
    "watch-test": "./node_modules/.bin/jest --watchAll",
    "react-devtools": "react-devtools",
    "eslint": "./node_modules/.bin/eslint --ext .jsx --ext .js src",
    "eslint-fix": "./node_modules/.bin/eslint --ext .jsx --ext .js --fix src",
    "eslint-report": "./node_modules/.bin/eslint -f node_modules/eslint-html-reporter/reporter.js -o report.html --ext .jsx --ext .js src || true",
    "sass-lint": "./node_modules/.bin/sass-lint -v -q",
    "sass-lint-report": "./node_modules/.bin/sass-lint -v -q -f JSON | node ./node_modules/scss-lint-html-reporter/bin/reporter.js"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "6",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.5.0",
    "eslint-html-reporter": "^0.5.2",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.5.0",
    "react-test-renderer": "^16.4.2",
    "webpack": "^1.12.13",
    "webpack-bundle-tracker": "0.0.93",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "css-loader": "^0.28.7",
    "es6-promise": "^4.2.4",
    "file-loader": "^1.1.6",
    "image-webpack-loader": "^3.4.2",
    "imagemin-optipng": "^5.2.1",
    "imagemin-svgo": "^6.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jest-cli": "^23.5.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.10",
    "mock-socket": "^8.0.2",
    "node-sass": "^4.9.3",
    "prop-types": "^15.6.2",
    "radium": "^0.24.1",
    "raf": "^3.4.0",
    "react": "^16.4.2",
    "react-cookie": "^3.0.4",
    "react-csv": "^1.0.15-RC2",
    "react-dom": "^16.4.2",
    "react-input-toggle": "^0.4.0",
    "react-redux": "^5.0.7",
    "react-slick": "^0.16.0",
    "react-table": "^6.8.6",
    "react-websocket": "^1.2.0",
    "redux": "^4.0.0",
    "redux-devtools": "^3.4.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "resolve-url": "^0.2.1",
    "resolve-url-loader": "^2.3.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "scss-lint-html-reporter": "^0.3.2",
    "scss-loader": "^0.0.1",
    "slick-carousel": "^1.8.1",
    "style-loader": "^0.23.0",
    "yarn": "^1.9.4"
  },
  "jest": {
    "globals": {
      "__PLATFORM__": "test"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.js",
    "modulePaths": [
      "node_modules",
      "src"
    ],
    "clearMocks": true,
    "verbose": true,
    "coverageReporters": [
      "html",
      "text",
      "text-summary"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/timeout-transition-group",
      "<rootDir>/node_modules/expect",
      "<rootDir>/node_modules/classnames",
      "<rootDir>/node_modules/sinon",
      "<rootDir>/node_modules/redux",
      "<rootDir>/node_modules/redux-thunk",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-tools",
      "<rootDir>/node_modules/react-devtools",
      "react",
      "enzyme",
      "jest-enzyme"
    ],
    "modulePathIgnorePatterns": [
      "/node_modules/",
      "jest-setup.js"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}",
      "src/containers/**/*.{js,jsx}",
      "src/utils/**/*.js",
      "src/actions/**/*.js",
      "src/reducers/**/*.js",
      "src/store/**/*.js"
    ]
  }
}
